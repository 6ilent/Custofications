<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>

		<dict>
			<key>cellClass</key><string>HBTwitterCell</string>
			<key>label</key><string>6ilent</string>
			<key>user</key><string>Zer0Entry</string>
		</dict>



		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Toggle me on or off!</string>
		</dict>
		<dict>
			<key>cell</key><string>PSSwitchCell</string>
			<key>default</key><true/>
			<key>defaults</key><string>com.6ilent.custofications</string>
			<key>label</key><string>Enable!</string>
			<key>key</key><string>cstEnabled</string>
			<key>PostNotification</key><string>com.6ilent.custofications/ReloadPrefs</string>
		</dict>



		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Customize me!</string>
		</dict>
		<dict>
			<key>bundle</key><string>AppList</string>
			<key>cell</key><string>PSLinkCell</string>
			<key>isController</key><true/>
			<key>label</key><string>Application</string>
			<key>ALSingleEnabledMode</key><true/>
			<key>ALSettingsKey</key><string>cstApp</string>
			<key>ALChangeNotification</key><string>com.6ilent.custofications/ReloadPrefs</string>
			<key>ALSettingsPath</key><string>/var/mobile/Library/Preferences/com.6ilent.custofications.plist</string>
			<key>ALSectionDescriptors</key>
			<array>
				<dict>
					<key>title</key><string>System Applications</string>
					<key>predicate</key><string>isSystemApplication = TRUE</string>
					<key>icon-size</key><real>29</real>
					<key>suppress-hidden-apps</key><true/>
					<key>cell-class-name</key><string>ALCheckCell</string>
				</dict>
				<dict>
					<key>title</key><string>User Applications</string>
					<key>predicate</key><string>isSystemApplication = FALSE</string>
					<key>icon-size</key><real>29</real>
					<key>suppress-hidden-apps</key><true/>
					<key>cell-class-name</key><string>ALCheckCell</string>
					<key>footer-title</key><string>Custofications by 6ilent</string>
				</dict>
			</array>
		</dict>
		<dict>
			<key>cell</key><string>PSEditTextCell</string>
			<key>default</key><string>Title</string>
			<key>defaults</key><string>com.6ilent.custofications</string>
			<key>detail</key><string>PSDetailController</string>
			<key>key</key><string>cstTitle</string>
			<key>label</key><string>Message Title: </string>
			<key>PostNotification</key><string>com.6ilent.custofications/ReloadPrefs</string>
		</dict>
		<dict>
			<key>cell</key><string>PSEditTextCell</string>
			<key>default</key><string>Hello World!</string>
			<key>defaults</key><string>com.6ilent.custofications</string>
			<key>detail</key><string>PSDetailController</string>
			<key>key</key><string>cstMessage</string>
			<key>label</key><string>Message Content: </string>
			<key>PostNotification</key><string>com.6ilent.custofications/ReloadPrefs</string>
		</dict>



		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Test me!</string>
		</dict>
		<dict>
			<key>action</key><string>sendNotifications:</string>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Send Notification</string>
		</dict>
		<dict>
			<key>action</key><string>sendBanner:</string>
			<key>cell</key><string>PSButtonCell</string>
			<key>label</key><string>Send Banner</string>
		</dict>



		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>label</key><string>Reset preferences</string>
		</dict>
		<dict>
			<key>action</key><string>resetPrefs:</string>
			<key>cell</key><string>PSButtonCell</string>
			<key>isDestructive</key><true/>
			<key>confirmation</key>
			<dict>
				<key>prompt</key>
				<string>Are you sure you want to reset the preferences?</string>
				<key>title</key>
				<string>Yes</string>
				<key>okTitle</key>
				<string>Yes</string>
				<key>cancelTitle</key>
				<string>Cancel</string>
			</dict>
			<key>label</key><string>Reset Preferences</string>
		</dict>



		<dict>
			<key>cell</key><string>PSGroupCell</string>
			<key>footerText</key><string>Custofications by 6ilent</string>
			<key>isStaticText</key><true/>
		</dict>
		<dict>
			<key>cellClass</key><string>HBLinkTableCell</string>
			<key>label</key><string>GitHub</string>
			<key>subtitle</key><string>Source Code</string>
			<key>url</key><string>https://github.com/6ilent/Custofications</string>
		</dict>
		<dict>
			<key>cellClass</key><string>HBLinkTableCell</string>
			<key>label</key><string>Donate to 6ilent</string>
			<key>subtitle</key><string>Support me!</string>
			<key>url</key><string>https://paypal.me/6ilent</string>
		</dict>

	</array>
	<key>title</key>
	<string>Custofications</string>
</dict>
</plist>
